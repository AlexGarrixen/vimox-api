{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "dist/index.js"
    }
  ],
  "env": {
    "DB_NAME": "@gxanime-api_db-prod-name",
    "DB_USER": "@gxanime-api_db-user",
    "DB_PASSWORD": "@gxanime-api_db-password",
    "DB_HOST": "@gxanime-api_db-host",
    "ORIGIN_CLIENT": "@gx-anime-api-origin_client",
    "SENDGRID_API_KEY": "@gx-anime-api-sengrid_api_key",
    "SECRET_JWT": "@gx-anime-api-secret_jwt",
    "SECRET_REFRESH_JWT": "@gx-anime-api-secret-refresh-jwt"
  }
}
